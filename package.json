{
  "name": "trollo",
  "version": "0.1.0",
  "description": "Trello viewer with current active and top backlog preview functionality",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/luke-biel/trollo.git"
  },
  "author": "≈Åukasz Biel <lukasz.p.biel@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luke-biel/trollo/issues"
  },
  "homepage": "https://github.com/luke-biel/trollo#readme",
  "scripts": {
    "build": "tsc -p .",
    "clean": "rm -rf ./lib",
    "clean-build": "npm run clean && npm run build",
    "app": "npm run build && node ./lib/index.js",
    "test": "npm run app",
    "refresh-pkg": "rm -rf ./node_modules ./package-lock.json && npm install",
    "preinstall": "npm run clean",
    "check": "eslint ./src/**/*.ts",
    "fix": "eslint ./src/**/*.ts --fix"
  },
  "main": "./lib/index.js",
  "bin": {
    "trollo": "./lib/index.js"
  },
  "dependencies": {
    "@types/clear": "^0.1.0",
    "@types/figlet": "^1.2.0",
    "@types/table": "^6.0.0",
    "axios": "^0.20.0",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "commander": "^6.1.0",
    "figlet": "^1.5.0",
    "path": "^0.12.7",
    "table": "^6.0.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
